      },
      "t3_uptake": {
        "label": "T3摂取率 (T3RU) [%]"
      }
    },
    "output": {
      "ft4_index": {
        "label": "遊離T4指数"
      }
    }
  },
  "t3_t4_ratio": {
    "name": "T3/T4比",
    "input": {
      "t3": {
        "label": "T3 [ng/dL]"
      },
      "t4": {
        "label": "T4 [µg/dL]"
      }
    },
    "output": {
      "t3_t4_ratio": {
        "label": "T3/T4比"
      },
      "interpretation": {
        "label": "判定"
      },
      "clinical_note": {
        "label": "臨床的意義",
