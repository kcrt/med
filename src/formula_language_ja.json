      }
    }
  },
  "jones_criteria_low_risk": {
    "name": "Jones基準 リウマチ熱診断 (低リスク)",
    "output": {
      "major_count": {
        "label": "主要基準数"
      },
      "minor_count": {
        "label": "副次基準数"
      },
      "diagnosis": {
        "label": "診断"
      },
      "population": {
