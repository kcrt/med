    "output": {
      "total_score": {
        "label": "PEWS合計スコア"
      },
      "risk_level": {
        "label": "リスクレベル"
      },
      "action": {
        "label": "推奨対応"
      },
      "purpose": {
        "label": "目的",
        "text": "入院小児の臨床悪化の早期発見、迅速対応チーム(RRT)のトリガー"
