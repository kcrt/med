    }
  },
  "phoenix_sepsis_score": {
    "name": "Phoenix敗血症スコア (2024)",
    "input": {
      "respiratory_score": {
        "label": "呼吸スコア"
      },
      "cardiovascular_points": {
        "label": "循環ポイント (0-6)"
      },
      "coagulation_score": {
        "label": "凝固スコア"
      },
      "neurologic_score": {
        "label": "神経スコア"
      }
    },
    "output": {
      "phoenix_score": {
        "label": "Phoenix合計スコア"
      },
