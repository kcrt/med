{
  "_meta": {
    "language": "ja",
    "version": "1.0.0",
    "lastModified": "2025-01-25"
  },
  "bmi_adult": {
    "name": "BMI (成人)",
    "input": {
      "height": {
        "label": "身長[cm]"
      },
      "weight": {
        "label": "体重[kg]"
      }
    },
    "output": {
      "who": {
        "label": "WHO(世界保健機関)",
        "text": "≧25: overwight, ≧30: obese"
      },
      "who_diag": {
        "label": "WHO(世界保健機関)"
      },
      "jasso": {
        "label": "日本肥満学会",
        "text": "＜18.5: 低体重, 22: 標準体重,≧25: 肥満"
      },
      "jasso_diag": {
        "label": "日本肥満学会",
        "formula": "iif(BMI<18.5, '低体重', BMI<25, '標準', '肥満')"
      }
    },
    "test": [
      {
        "input": {
          "height": 170,
          "weight": 70
        },
        "output": {
          "BMI": 24.2,
          "who_diag": "normal",
          "jasso_diag": "標準"
        }
      }
    ]
  },
  "bmi_child": {
    "name": "BMI (小児)",
    "input": {
      "height": {
        "label": "身長[cm]"
      },
      "weight": {
        "label": "体重[kg]"
      }
    },
    "output": {
      "kaup": {
        "label": "Kaup指数"
      },
      "info1": {
        "label": "乳幼児(3ヶ月以降)",
        "text": "正常範囲: 15-18"
      },
      "kaup_diag": {
        "label": "乳幼児(3ヶ月以降)",
        "formula": "if(kaup>=15 and kaup<=18, '正常範囲', '正常範囲外')"
      },
      "rohrer": {
        "label": "Rohrer指数"
      },
      "info2": {
        "label": "学童",
        "text": "標準: 130, 160以上で肥満"
      },
      "rohrer_diag": {
        "label": "学童",
        "formula": "iif(rohrer<130, '痩身', rohrer<160, '標準', '肥満')"
      }
    },
    "test": [
      {
        "input": {
          "height": 80,
          "weight": 10
        },
        "output": {
          "kaup": 15.6,
          "kaup_diag": "正常範囲",
          "rohrer": 195.3,
          "rohrer_diag": "肥満"
        }
      }
    ]
  },
  "postconceptional_days": {
    "name": "在胎週数・修正月齢",
    "input": {
      "edc": {
        "label": "出生予定日"
      },
      "target_date": {
        "label": "日付"
      }
    },
    "output": {
      "postconceptional_days": {
        "label": "受胎日数"
      },
      "postconceptional_age": {
        "label": "在胎週数"
      },
      "corrected_age": {
        "label": "修正月齢"
      }
    }
  },
  "ckd_epi_2021": {
    "name": "CKD-EPI 2021 (クレアチニン・シスタチンC)",
    "input": {
      "scr": {
        "label": "血清クレアチニン [mg/dL]"
      },
      "cystatin_c": {
        "label": "血清シスタチンC [mg/L]"
      },
      "age": {
        "label": "年齢 [歳]"
      },
      "sex": {
        "label": "性別"
      }
    },
    "output": {
      "egfr_2021": {
        "label": "推算GFR [mL/min/1.73m²] (CKD-EPI 2021)"
      }
    }
  },
  "abcd2i": {
    "name": "ABCD²-Iスコア",
    "input": {
      "age": {
        "label": "年齢≥60歳"
      },
      "bp": {
        "label": "血圧≥140/90 mmHg"
      },
      "clinical": {
        "label": "臨床症状"
      },
      "duration": {
        "label": "持続時間"
      },
      "diabetes": {
        "label": "糖尿病"
      },
      "dwi": {
        "label": "DWI病変あり"
      }
    },
    "output": {
      "score": {
        "label": "スコア"
      },
      "risk": {
        "label": "リスク",
        "formula": "iif(score>=7, '超高リスク', score>=5, '高リスク', score>=3, '中リスク', '低リスク')"
      },
      "note": {
        "label": "備考",
        "text": "ABCD²-IはDWI画像診断を追加。スコア範囲: 0-9点。DWI病変ありで2点。"
      }
    }
  },
  "gcs": {
    "name": "Glasgow Coma Scale (GCS)",
    "input": {
      "eye": {
        "label": "開眼",
        "options": [
          {
            "value": 4,
            "label": "4 - 自発的に開眼"
          },
          {
            "value": 3,
            "label": "3 - 呼びかけで開眼"
          },
          {
            "value": 2,
            "label": "2 - 痛刺激で開眼"
          },
          {
            "value": 1,
            "label": "1 - 開眼なし"
          }
        ]
      },
      "verbal": {
        "label": "言語",
        "options": [
          {
            "value": 5,
            "label": "5 - 見当識正常"
          },
          {
            "value": 4,
            "label": "4 - 会話混乱"
          },
          {
            "value": 3,
            "label": "3 - 不適切な言葉"
          },
          {
            "value": 2,
            "label": "2 - 音声のみ（理解不能）"
          },
          {
            "value": 1,
            "label": "1 - 発語なし"
          }
        ]
      },
      "motor": {
        "label": "運動",
        "options": [
          {
            "value": 6,
            "label": "6 - 命令に従う"
          },
          {
            "value": 5,
            "label": "5 - 痛刺激で定位"
          },
          {
            "value": 4,
            "label": "4 - 痛刺激で退避（正常屈曲）"
          },
          {
            "value": 3,
            "label": "3 - 異常屈曲（除皮質強直）"
          },
          {
            "value": 2,
            "label": "2 - 伸展（除脳強直）"
          },
          {
            "value": 1,
            "label": "1 - なし"
          }
        ]
      }
    },
    "output": {
      "total": {
        "label": "GCSスコア"
      },
      "severity": {
        "label": "重症度",
        "formula": "iif(total>=13, '軽症', total>=9, '中等症', '重症')"
      },
      "interpretation": {
        "label": "解釈",
        "text": "13-15点: 軽症, 9-12点: 中等症, 3-8点: 重症（昏睡）, 3点: 深昏睡/脳死"
      }
    },
    "test": [
      {
        "input": {
          "eye": 4,
          "verbal": 5,
          "motor": 6
        },
        "output": {
          "total": 15,
          "severity": "軽症"
        }
      },
      {
        "input": {
          "eye": 1,
          "verbal": 1,
          "motor": 2
        },
        "output": {
          "total": 4,
          "severity": "重症"
        }
      }
    ]
  },
  "sofa": {
    "name": "SOFAスコア (敗血症関連臓器障害評価)",
    "input": {
      "pao2": {
        "label": "PaO₂ [mmHg]"
      },
      "fio2": {
        "label": "FiO₂ (0-1)"
      },
      "platelets": {
        "label": "血小板数 [×10³/μL]"
      },
      "bilirubin": {
        "label": "総ビリルビン [mg/dL]"
      },
      "map": {
        "label": "平均動脈圧 [mmHg]"
      },
      "vasopressor": {
        "label": "血管作動薬使用",
        "options": [
          {
            "value": 0,
            "label": "なし"
          },
          {
            "value": 1,
            "label": "ドパミン≤5 μg/kg/min または ドブタミン"
          },
          {
            "value": 2,
            "label": "ドパミン>5 μg/kg/min または エピネフリン≤0.1 μg/kg/min または ノルエピネフリン≤0.1 μg/kg/min"
          },
          {
            "value": 3,
            "label": "ドパミン>15 μg/kg/min または エピネフリン>0.1 μg/kg/min または ノルエピネフリン>0.1 μg/kg/min"
          }
        ]
      },
      "gcs": {
        "label": "Glasgow Coma Scale"
      },
      "creatinine": {
        "label": "クレアチニン [mg/dL]"
      },
      "urine_output": {
        "label": "尿量 [mL/day]",
        "default": 0
      }
    },
    "output": {
      "pao2_fio2": {
        "label": "PaO₂/FiO₂比"
      },
      "respiration_score": {
        "label": "呼吸器スコア"
      },
      "coagulation_score": {
        "label": "凝固スコア"
      },
      "liver_score": {
        "label": "肝臓スコア"
      },
      "cardiovascular_score": {
        "label": "循環器スコア"
      },
      "cns_score": {
        "label": "中枢神経スコア"
      },
      "renal_score": {
        "label": "腎臓スコア"
      },
      "total": {
        "label": "SOFAスコア合計"
      },
      "interpretation": {
        "label": "解釈",
        "text": "≧2点: 臓器障害を示唆; スコア変化≧2点: 臓器機能の有意な変化を示す"
      }
    }
  },
  "qsofa": {
    "name": "qSOFAスコア (quick SOFA)",
    "input": {
      "respiratory_rate": {
        "label": "呼吸数 [/分]"
      },
      "altered_mentation": {
        "label": "意識障害 (GCS<15)"
      },
      "systolic_bp": {
        "label": "収縮期血圧 [mmHg]"
      }
    },
    "output": {
      "score": {
        "label": "qSOFAスコア"
      },
      "risk": {
        "label": "リスク評価",
        "formula": "iif(score >= 2, '高リスク - 敗血症を考慮', '低リスク')"
      },
      "interpretation": {
        "label": "解釈",
        "text": "スコア≧2点: 予後不良リスクが高い（死亡率≧10%）；敗血症の評価を要する"
      },
      "note": {
        "label": "備考",
        "text": "qSOFAは、ICU外で感染疑い例のうち予後不良リスクが高い症例を早期同定するためのベッドサイドスクリーニングツール"
      }
    }
  },
  "corrected_sodium_katz": {
    "name": "補正ナトリウム (高血糖時) - Katz式",
    "input": {
      "measured_na": {
        "label": "測定ナトリウム [mmol/L]"
      },
      "glucose": {
        "label": "血糖 [mg/dL]"
      }
    },
    "output": {
      "corrected_na": {
        "label": "補正ナトリウム [mmol/L]"
      },
      "interpretation": {
        "label": "判定"
      },
      "clinical_note": {
        "label": "臨床的意義",
        "text": "DKAにおいて、補正Naが8時間で約5 mmol/L上昇しない場合、脳浮腫のリスクを示す"
      }
    }
  },
  "serum_osmolality": {
    "name": "血清浸透圧 (計算値)",
    "input": {
      "sodium": {
        "label": "ナトリウム [mmol/L]"
      },
      "glucose": {
        "label": "血糖 [mg/dL]"
      },
      "bun": {
        "label": "BUN [mg/dL]"
      }
    },
    "output": {
      "osmolality": {
        "label": "計算浸透圧 [mOsm/kg]"
      },
      "interpretation": {
        "label": "判定"
      },
      "pediatric_note": {
        "label": "小児基準値",
        "text": "小児正常範囲: 275-290 mOsm/kg"
      }
    }
  },
  "anion_gap": {
    "name": "アニオンギャップ",
    "input": {
      "sodium": {
        "label": "ナトリウム [mmol/L]"
      },
      "chloride": {
        "label": "クロール [mmol/L]"
      },
      "bicarbonate": {
        "label": "重炭酸 [mmol/L]"
      }
    },
    "output": {
      "anion_gap": {
        "label": "アニオンギャップ [mEq/L]"
      },
      "interpretation": {
        "label": "判定"
      }
    }
  },
  "fena": {
    "name": "尿中ナトリウム排泄率 (FENa)",
    "input": {
      "urine_na": {
        "label": "尿中ナトリウム [mmol/L]"
      },
      "serum_na": {
        "label": "血清ナトリウム [mmol/L]"
      },
      "urine_cr": {
        "label": "尿中クレアチニン [mg/dL]"
      },
      "serum_cr": {
        "label": "血清クレアチニン [mg/dL]"
      }
    },
    "output": {
      "fena": {
        "label": "FENa [%]"
      },
      "interpretation": {
        "label": "判定"
      },
      "limitation": {
        "label": "注意点",
        "text": "利尿薬使用時は信頼性が低下 → FEUreaを使用"
      }
    }
  },
  "reticulocyte_production_index": {
    "name": "網状赤血球産生指数 (RPI)",
    "input": {
      "reticulocyte_percent": {
        "label": "網状赤血球 [%]"
      },
      "patient_hct": {
        "label": "患者ヘマトクリット [%]"
      }
    },
    "output": {
      "reticulocyte_index": {
        "label": "網状赤血球指数 (RI)"
      },
      "rpi": {
        "label": "網状赤血球産生指数 (RPI)"
      },
      "interpretation": {
        "label": "判定"
      }
    }
  },
  "phototherapy_threshold": {
    "name": "光線療法開始基準 (AAP 2022)",
    "input": {
      "bilirubin": {
        "label": "総ビリルビン [mg/dL]"
      },
      "age_hours": {
        "label": "日齢 [時間]"
      },
      "gestational_age": {
        "label": "在胎週数 [週]"
      },
      "has_risk_factors": {
        "label": "リスク因子あり"
      }
    },
    "output": {
      "threshold_adjusted": {
        "label": "調整後基準値 [mg/dL]"
      },
      "recommendation": {
        "label": "推奨"
      },
      "risk_factors": {
        "label": "リスク因子",
        "text": "在胎38週未満、アルブミン<3.0 g/dL、同種免疫性溶血性疾患(DAT陽性)、G6PD欠損症、敗血症、臨床的不安定"
      }
    }
  },
  "pews_brighton": {
    "name": "小児早期警告スコア (PEWS) - Brighton版",
