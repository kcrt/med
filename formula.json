		"ccr_adult" : {
			"input": {
				"weight": {"name": "体重[kg]", "type": "float", "min": 3, "max": 300},
				"age" : {"name": "年齢", "type": "float", "min": 17, "max": 100},
				"scr" : {"name": "血中Cr値[mg/dL]", "type": "float", "min": 0.001, "max": 20},
				"sex" : {"name": "性別", "type": "sex"},
				"info" : {"name": "注意事項", "type": "info", "text": "成人に対して使用してください。"}
			},
			"output": {
				"cockcroft": {"name": "CCr[mL/min] (Cockcroft-Gault)", "formula" : "sex==1 ? ((140-age) * weight) / (72 * scr) : (((140-age) * weight) / (72 * scr)) * 0.85", "toFixed": 4},
				"mdrd_ord" : {"name" : "GFR[mL/min/1.73m²] (MDRD原法)", "formula": "sex==1 ? 186 * Math.pow(age, -0.203) * Math.pow(scr, -1.154) : (186 * Math.pow(age, -0.203) * Math.pow(scr, -1.154)) * 0.742", "toFixed": 4},
				"mdrd_jp" : {"name" : "GFR[mL/min/1.73m²] (日本人向けMDRD)", "formula": "sex==1 ? 175 * Math.pow(age, -0.203) * Math.pow(scr, -1.154) * 0.741 : (175 * Math.pow(age, -0.203) * Math.pow(scr, -1.154)) * 0.741 * 0.742", "toFixed": 4},
				"jp_egfr" : {"name" : "GFR[mL/min/1.73m²] (日本人のGFR推算式プロジェクト)", "formula": "sex==1 ? 194 * Math.pow(age, -0.287) * Math.pow(scr, -1.094) : (194 * Math.pow(age, -0.287) * Math.pow(scr, -1.094)) * 0.739", "toFixed": 4}
			}
		},
		"ccr": {
			"input": {
				"height": {"name": "身長[cm]", "type": "float", "min": 10, "max": 300},
				"weight": {"name": "体重[kg]", "type": "float", "min": 0, "max": 300},
				"ucr": {"name": "尿中クレアチニン[mg/dL]", "type": "float", "min": 0.001, "max": 1000},
				"scr": {"name": "血清クレアチニン[mg/dL]", "type": "float", "min": 0.001, "max": 100},
				"vday": {"name": "一日尿量[mL/day]", "type": "float", "min": 1, "max": 50000},
				"info": {"name": "注意", "type": "info", "text": "尿量は24時間量を入力してください。"}
			},
			"output": {
				"ccr": {"name": "CCr[ml/min/1.73m²]", "formula": "ucr * (vday/24/60) / scr * 1.73 / BSA_DuBois(height, weight)"}
			},
			"ref": {
				"Wikipedia": "http://ja.wikipedia.org/wiki/%E3%82%AF%E3%83%AC%E3%82%A2%E3%83%81%E3%83%8B%E3%83%B3%E3%82%AF%E3%83%AA%E3%82%A2%E3%83%A9%E3%83%B3%E3%82%B9"
			}
		},
		"cysc": {
			"input": {
				"cysc": {"name": "血清シスタチンC値", "type": "float", "min": 0, "max": 100}
			},
			"output": {
				"hoek": {"name": "eGFR[mL/min/1.73m²] (Hoek)", "formula": "80.35/cysc-4.32"},
				"grubb_a": {"name": "eGFR[mL/min/1.73m²] (Grubb 14才以上)", "formula": "84.69*Math.pow(cysc, -1.680)"},
				"grubb_c": {"name": "eGFR[mL/min/1.73m²] (Grubb 14才未満)", "formula": "84.69*Math.pow(cysc,-1.680)*1.384"},
				"filler": {"name": "eGFR[mL/min/1.73m²] (Filler; 小児用)", "formula": "91.62 * Math.pow(cysc, -1.123)"}
			},
			"ref": {
				"Nephrol Dial Transplant. 2003 Oct;18(10):2024-31.": "pubmed:13679476",
				"Nephrol Dial Transplant. 2008 Jan;23(1):154-60. Epub 2007 Oct 2.":"pubmed:17911090"
			}
		},
		"ccr_child": {
			"input": {
				"height": {"name": "身長[cm]", "type": "float", "min": 3, "max": 300},
				"scr" : {"name": "血中Cr値(酵素法)[mg/dL]", "type": "float", "min": 0.001, "max": 20},
				"k" : {"name": "年齢", "type": "select", "item": {"低出生体重児": 0.33,"正常体重児": 0.45, "2〜12歳": 0.55, "女児(13-21歳)": 0.55, "男児(13-21歳)": 0.7}}
			},
			"output": {
				"shwaltz": {"name": "eGFR[mL/min/1.73m²] (Schwaltz)", "formula": "k*height/(scr+0.2)"},
				"updatedshwaltz": {"name": "eGFR[mL/min/1.73m2] (Schwartz 2009; 7.7-14.3歳)", "formula": "0.413 * height / scr"}
			},
			"ref": {
				"Minds" : "http://minds.jcqhc.or.jp/n/med/4/med0067/G0000189/0142",
				"New Equations to Estimate GFR in Children with CKD": "pubmed:19158356"
			}
		},
		"fena": {
			"input": {
				"SNa": {"name": "血清Na[mEq/L]", "type": "float", "min": 0, "max": 1000},
				"SCre": {"name": "血清Cre[mg/dL]", "type": "float", "min": 0, "max": 1000},
				"UNa": {"name": "尿Na[mEq/L]", "type": "float", "min": 0, "max": 1000},
				"UCre": {"name": "尿Cre[mg/dL]", "type": "float", "min": 0, "max": 1000}
			},
			"output": {
				"fena": {"name": "FENa[%]", "formula": "(UNa*SCre)/(SNa*UCre)*100"},
				"info": {"name": "情報", "text": "正常では<1%。腎性腎不全では>1%"}
			}
		},
		"ptrp": {
			"input": {
				"SP": {"name": "血清P[mg/dL]", "type": "float", "min": 0, "max": 1000},
				"SCre": {"name": "血清Cre[mg/dL]", "type": "float", "min": 0, "max": 1000},
				"UP": {"name": "尿P[mg/dL]", "type": "float", "min": 0, "max": 1000},
				"UCre": {"name": "尿Cre[mg/dL]", "type": "float", "min": 0, "max": 1000}
			},
			"output": {
				"ptrp": {"name": "%TRP", "formula": "(1-(UP*SCre)/(SP*UCre))*100"}
			}
		},
		"AaDO2":{
			"input" : {
				"fio2": {"name": "FiO₂", "type": "float", "min": 0, "max": 1, "placeholder": "room air = 0.21"},
				"pao2": {"name": "PaO₂[mmHg]", "type": "float", "min": 0, "max": 1000},
				"paco2": {"name": "PaCO₂[mmHg]", "type": "float", "min": 0, "max": 1000}
			},
			"output": {
				"aado2": {"name": "A-aDO₂", "formula": "(760-47)*fio2-paco2/0.8-pao2"},
				"info": {"name": "情報", "text": "正常では10以下"}
			}
		},
		"psi": {
			"input": {
				 "age": {"name": "年齢", "type": "float", "min": 0, "max":150},
				 "sex": {"name": "性別", "type": "sex"},
				 "nursinghome":		{"name": "老人ホーム入所中", "type": "onoff"},
				 "info1":			{"name": "", "type": "info", "text": "合併症"},
				 "neoplastic":		{"name": "悪性腫瘍", "type": "onoff"},
				 "liver":			{"name": "肝疾患", "type": "onoff"},
				 "congestivehf":	{"name": "うっ血性心不全", "type": "onoff"},
				 "cerebrovasc":		{"name": "脳血管障害", "type": "onoff"},
				 "renal":			{"name": "腎疾患", "type": "onoff"},
				 "info2":			{"name": "", "type": "info", "text": "身体所見"},
				 "alteredmental":	{"name": "意識レベルの低下", "type": "onoff"},
				 "pulsege125":		{"name": "脈拍数 ≧125bpm", "type": "onoff"},
				 "rrgt30":			{"name": "呼吸数 ＞30bpm", "type": "onoff"},
				 "sbplt90":			{"name": "収縮期血圧 ＜90mmHg", "type": "onoff"},
				 "temp":			{"name": "体温 35℃未満/40℃以上", "type": "onoff"},
				 "info3":			{"name": "", "type": "info", "text": "検査所見"},
				 "apH":				{"name": "動脈血ガスpH ＜7.35", "type": "onoff"},
				 "bun":				{"name": "BUN ≧30mg/dL", "type": "onoff"},
				 "Na":				{"name": "Na ＜130mEq/L", "type": "onoff"},
				 "BS":				{"name": "血糖 ≧250mg/dL", "type": "onoff"},
				 "Ht":				{"name": "Ht ＜30%", "type": "onoff"},
				 "PaO2":			{"name": "PaO2 ＜60mmHg", "type": "onoff"},
				 "pleuraleffusion":	{"name": "胸水", "type": "onoff"}
			},
			"output": {
				"point": {"name": "点数", "formula": "(sex ? age : age - 10) + nursinghome*10 + neoplastic*30 + liver*20 + congestivehf*10 + cerebrovasc*10 + renal*10 + alteredmental*20 + pulsege125*20 + rrgt30*20 + sbplt90*15 + temp*10 + apH*30 + bun*20 + Na*20 + BS*10 + Ht*10 + PaO2*10 + pleuraleffusion*10", "toFixed": ""},
				"aclass": {"name": "クラス", "code": "if(!(age>50 || alteredmental || pulsege125 || rrgt30 || sbplt90 || temp || neoplastic || congestivehf || cerebrovasc || renal || liver )){return 'I';}else if(point < 70){return 'II';}else if(point <= 90){return 'III';}else if(point <=130){return 'IV';}else{return 'V';}"},
				"deathrate": {"name": "死亡率(PORT Study)[%]", "code": "if(aclass=='I'){return 0.1;}else if(aclass=='II'){return 0.6;}else if(aclass=='III'){return 0.9;}else if(aclass=='IV'){return 9.3;}else{return '27.0';}", "toFixed": ""}
			},
			"ref": {
				"Wikipedia": "http://en.wikipedia.org/wiki/Pneumonia_severity_index"
			}
		},
		"adrop" : {
			"input": {
				"a": {"name": "男70歳以上/女75歳以上", "type": "onoff"},
				"d" : {"name": "BUN ≧21または脱水あり", "type": "onoff"},
				"r" : {"name": "SpO₂≦90%(PaO₂≦60Torr)", "type": "onoff"},
				"o" : {"name": "意識障害", "type": "onoff"},
				"p": {"name": "sBP≦90mmHg", "type": "onoff"}
			},
			"output": {
				"point": {"name": "該当項目数", "formula": "a+d+r+o+p", "toFixed": ""},
				"judge": {"name": "判定", "code" : "if(point==0){return '外来';}else if(point<=2){return '外来または入院'}else if(point==3){return '入院'}else{return 'ICU入院'}"},
				"info": {"name": "注意", "text": "ただし、ショックがあれば1項目のみでも超重症とする"}
			}
		},
		"veinbloodgas": {
			"input": {
				"pH": {"name": "pH", "type": "float", "min": 5, "max": 9},
				"pvCO2": {"name": "pvCO2", "type": "float", "min": 0, "max": 200},
				"pvHCO3": {"name": "pvHCO3-", "type": "float", "min": 0, "max": 80}
			},
			"output": {
				"apH": {"name": "動脈pH", "formula": "-0.307 + 1.05 * pH"},
				"paCO2": {"name": "paCO2", "formula": "0.805 + 0.936 * pvCO2"},
				"paHCO3": {"name": "paHCO3", "formula": "0.513 + 0.94 * pvHCO3"}
			},
			"ref": {
				"Agreement between central venous and arterial blood gas measurements in the intensive care unit." : "doi:10.2215/CJN.00330109"
			}
		},
		"abcd2" : {
			"input": {
				"a": {"name": "60歳以上", "type": "onoff"},
				"b": {"name": "sBP≧140 or dBP≧90mmHg", "type": "onoff"},
				"c": {"name": "臨床症状", "type" : "select", "item": {"片麻痺あり" : 2, "麻痺を伴わない構音障害": 1, "その他" : 0}},
				"d1": {"name": "持続時間", "type": "select", "item": {"60分以上": 2, "10-59分": 1, "10分未満": 0}},
				"d2": {"name": "糖尿病", "type": "onoff"}
			},
			"output" : {
				"point": {"name": "点数", "formula": "a+b+c+d1+d2"},
				"judge": {"name": "判定", "code": "var x=a+b+c+d1+d2; if(x>=6){return '大きな危険 二日以内の脳梗塞が8.1%';}else if(x>=4){return '中等度の危険 二日以内の脳梗塞が4.1%';}else{return '小さな危険 二日以内の脳梗塞が1.0%';}"}
			},
			"ref": {
				"Stroke Service; Lancet. 2007 Jan 27;369(9558):283-92." : "pubmed:17258668"
			}
		},
		"lumbardepth": {
			"input": {
				"height": {"name": "身長[cm]", "type": "float", "min": 30, "max": 300},
				"weight": {"name": "体重[kg]", "type": "float", "min": 3, "max": 300}
			},
			"output": {
				"adult": {"name": "成人[cm]", "formula": "1+17*(weight/height)"},
				"child_a": {"name": "小児(3ヶ月以降) [cm]", "formula": "1.3 + 0.07*weight"},
				"child_b": {"name": "小児(3ヶ月未満) [cm]", "formula": "height*0.03"}
			},
			"ref": {
				"Am J Emerg Med. 2005 Oct;23(6):742-6.": "pubmed:16182981",
				"Coll Antropol. 2003 Dec;27(2):623-6." : "pubmed:14746151",
				"Arch Dis Child. 1997 Nov;77(5):450.": "pubmed:9487973"
			}
		},
		"abrsd": {
			"input": {
				"I": {"name": "I波[ms]", "type": "float", "min": 0, "max": 10},
				"III": {"name": "III波[ms]", "type": "float", "min": 0, "max": 10},
				"V": {"name": "V波[ms]", "type": "float", "min": 0, "max": 10},
				"method": {"name": "測定時期", "type": "select", "item": {"在胎32, 33週": 0, "在胎34, 35週": 1, "在胎36, 37週": 2, "在胎38, 39週": 3, "在胎40, 41週": 4, "在胎42, 43週": 5, "在胎44, 45週": 6, "〜5ヶ月": 7, "6-11ヶ月": 8, "1歳": 9, "2-4歳": 10, "5-13歳": 11, "成人": 12}}
			},
			"output": {
				"I_avg": {"name": "hidden", "formula": "[2.4,2.43,2.38,2.12,1.98,1.96,1.86,1.6,1.9,1.5,1.5,1.4,1.5]"},
				"I_SD": {"name": "hidden", "formula": "[0.18,0.15,0.14,0.16,0.13,0.08,0.13,0.3,0.3,0.1,0.3,0.1,0.1]"},
				"III_avg": {"name": "hidden", "formula": "[5.49,5.45,5.16,5.16,4.94,4.86,4.71,4.6,4.3,3.9,3.9,3.8,3.8]"},
				"III_SD": {"name": "hidden", "formula": "[0.51,0.36,0.46,0.36,0.39,0.25,0.53,0.1,0.4,0.3,0.2,0.2,0.1]"},
				"V_avg": {"name": "hidden", "formula": "[8.04,7.84,7.4,7.33,7.04,6.97,6.61,6.5,6.5,5.9,5.7,5.7,5.8]"},
				"V_SD": {"name": "hidden", "formula": "[0.47,0.34,0.57,0.46,0.47,0.34,0.53,0.3,0.4,0.4,0.2,0.4,0.1]"},
				"I_III_avg": {"name": "hidden", "formula": "[-1,-1,-1,-1,-1,-1,-1,2.9,2.4,2.5,2.3,2.4,2.3]"},
				"I_III_SD": {"name": "hidden", "formula": "[-1,-1,-1,-1,-1,-1,-1,0.2,0.3,0.3,0.2,0.2,0.2]"},
				"I_V_avg": {"name": "hidden", "formula": "[5.54,5.35,5.14,5.06,5.05,5,4.91,5,4.5,4.5,4.2,4.4,4.3]"},
				"I_V_SD": {"name": "hidden", "formula": "[0.49,0.45,0.59,0.49,0.38,0.2,0.33,0.6,0.4,0.2,0.1,0.3,0.2]"},
				"I_avg_SD": {"name": "I波", "formula": "GetZScoreStr(I, I_avg[method], I_SD[method])"},
				"III_avg_SD": {"name": "III波", "formula": "GetZScoreStr(III, III_avg[method], III_SD[method])"},
				"V_avg_SD": {"name": "V波", "formula": "GetZScoreStr(V, V_avg[method], V_SD[method])"},
				"I_III_avg_SD": {"name": "I-III波", "formula": "I_III_avg[method] == -1 ? '' : GetZScoreStr(III - I, I_III_avg[method], I_III_SD[method])"},
				"I_V_avg_SD": {"name": "I-V波", "formula": "GetZScoreStr(V - I, I_V_avg[method], I_V_SD[method])"},
				"info1" : {"name": "音圧", "text": "新生児は 85dBHL, それ以降は 90dBHLのデータです。"}
			},
			"ref": {
				"聴性脳幹反応(Auditory Brainstem Response:ABR)の新生児期への応用 木下洋 関西医大誌 第35巻第3号, 1983": "",
				"小児科診療 2013増刊号 理解して出そう小児の検査―オーダー・手技・解釈": "isbn:4910047260434"
			}
		},
		"epidrug": {
			"type": "image",
			"src" : "image/epidrug.png",
			"ref": {
				"てんかん 小児科診療増刊号, 62; 436-439, 1999.": "",
				"小児の薬の選び方・使い方": "isbn:9784525284435"
			}
		},
		"lmsvalue": {
			"input": {
				"value": {"name": "値", "type": "float", "min": -99999999, "max": 99999999},
				"l": {"name": "L (歪度)", "type": "float", "min": -10, "max": 10},
				"m": {"name": "M (平均)", "type": "float", "min": -99999999, "max": 99999999},
